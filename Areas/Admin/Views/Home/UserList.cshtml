@using asp.netcoreIdentityApp.Web.Areas.Admin.Models
@{
    ViewData["Title"] ="UserList";
}
@model List<UserViewModel>
<h1>UserList</h1>

<table class="table table-striped">
    <thead> 
        <tr>
            <td>Id</td>
            <td>User Name</td>
            <td>Email</td>
        </tr>
    </thead>

    <tbody>

        @foreach(var item in Model){
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>






